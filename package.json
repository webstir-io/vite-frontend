{
  "name": "@webstir-io/vite-frontend",
  "version": "0.1.4",
  "description": "Vite-powered frontend module provider for Webstir workspaces.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./provider": {
      "types": "./dist/provider.d.ts",
      "import": "./dist/provider.js",
      "default": "./dist/provider.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "test": "node --test tests/*.test.js",
    "smoke": "node -e \"console.log('[smoke] no smoke tests configured');\"",
    "release": "bash scripts/publish.sh"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.18.1"
  },
  "license": "MIT",
  "author": "Webstir",
  "repository": {
    "type": "git",
    "url": "https://github.com/webstir-io/vite-frontend"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "dependencies": {
    "@webstir-io/module-contract": "^0.1.11",
    "glob": "^10.4.1",
    "vite": "^7.1.10"
  },
  "devDependencies": {
    "@types/node": "^20.19.21",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2"
  }
}
